<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled state -->
    <item android:drawable="@drawable/shape_orange_button_disabled_6" android:state_enabled="false" />

    <!-- Not pressed -->
    <item android:drawable="@drawable/shape_orange_button_6" android:state_pressed="false" />

    <!-- Pressed -->
    <item android:drawable="@drawable/shape_orange_button_disabled_6" android:state_pressed="true" />

    <!-- Default color -->
    <item android:drawable="@drawable/shape_orange_button_6" android:state_enabled="true" />
</selector>